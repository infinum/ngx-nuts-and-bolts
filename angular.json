{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"projects": {
		"environment-variables-example-app-base": {
			"projectType": "library",
			"root": "libs/environment-variables-example-app-base",
			"sourceRoot": "libs/environment-variables-example-app-base/src",
			"prefix": "ngx-nuts-and-bolts",
			"architect": {
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/libs/environment-variables-example-app-base"],
					"options": {
						"jestConfig": "libs/environment-variables-example-app-base/jest.config.ts",
						"passWithNoTests": true
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"libs/environment-variables-example-app-base/src/**/*.ts",
							"libs/environment-variables-example-app-base/src/**/*.html"
						]
					}
				}
			},
			"tags": []
		},
		"environment-variables-fetch-example": {
			"projectType": "application",
			"root": "apps/environment-variables-fetch-example",
			"sourceRoot": "apps/environment-variables-fetch-example/src",
			"prefix": "ngx-nuts-and-bolts",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"outputs": ["{options.outputPath}"],
					"options": {
						"outputPath": "dist/apps/environment-variables-fetch-example",
						"index": "apps/environment-variables-fetch-example/src/index.html",
						"main": "apps/environment-variables-fetch-example/src/main.ts",
						"polyfills": "apps/environment-variables-fetch-example/src/polyfills.ts",
						"tsConfig": "apps/environment-variables-fetch-example/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"apps/environment-variables-fetch-example/src/favicon.ico",
							"apps/environment-variables-fetch-example/src/assets"
						],
						"styles": ["apps/environment-variables-fetch-example/src/styles.scss"],
						"scripts": []
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "apps/environment-variables-fetch-example/src/environments/environment.ts",
									"with": "apps/environment-variables-fetch-example/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "environment-variables-fetch-example:build:production"
						},
						"development": {
							"browserTarget": "environment-variables-fetch-example:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "environment-variables-fetch-example:build"
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"apps/environment-variables-fetch-example/src/**/*.ts",
							"apps/environment-variables-fetch-example/src/**/*.html"
						]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/apps/environment-variables-fetch-example"],
					"options": {
						"jestConfig": "apps/environment-variables-fetch-example/jest.config.ts",
						"passWithNoTests": true
					}
				}
			},
			"tags": []
		},
		"environment-variables-ssr-example": {
			"projectType": "application",
			"root": "apps/environment-variables-ssr-example",
			"sourceRoot": "apps/environment-variables-ssr-example/src",
			"prefix": "ngx-nuts-and-bolts",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"outputs": ["{options.outputPath}"],
					"options": {
						"outputPath": "dist/environment-variables-ssr-example/browser",
						"index": "apps/environment-variables-ssr-example/src/index.html",
						"main": "apps/environment-variables-ssr-example/src/main.ts",
						"polyfills": "apps/environment-variables-ssr-example/src/polyfills.ts",
						"tsConfig": "apps/environment-variables-ssr-example/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"apps/environment-variables-ssr-example/src/favicon.ico",
							"apps/environment-variables-ssr-example/src/assets"
						],
						"styles": ["apps/environment-variables-ssr-example/src/styles.scss"],
						"scripts": []
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "apps/environment-variables-ssr-example/src/environments/environment.ts",
									"with": "apps/environment-variables-ssr-example/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "environment-variables-ssr-example:build:production"
						},
						"development": {
							"browserTarget": "environment-variables-ssr-example:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "environment-variables-ssr-example:build"
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"apps/environment-variables-ssr-example/src/**/*.ts",
							"apps/environment-variables-ssr-example/src/**/*.html"
						]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/apps/environment-variables-ssr-example"],
					"options": {
						"jestConfig": "apps/environment-variables-ssr-example/jest.config.ts",
						"passWithNoTests": true
					}
				},
				"server": {
					"builder": "@angular-devkit/build-angular:server",
					"options": {
						"outputPath": "dist/environment-variables-ssr-example/server",
						"main": "apps/environment-variables-ssr-example/server.ts",
						"tsConfig": "apps/environment-variables-ssr-example/tsconfig.server.json",
						"inlineStyleLanguage": "scss"
					},
					"configurations": {
						"production": {
							"outputHashing": "media",
							"fileReplacements": [
								{
									"replace": "apps/environment-variables-ssr-example/src/environments/environment.ts",
									"with": "apps/environment-variables-ssr-example/src/environments/environment.prod.ts"
								}
							]
						},
						"development": {
							"optimization": false,
							"sourceMap": true,
							"extractLicenses": false
						}
					},
					"defaultConfiguration": "production"
				},
				"serve-ssr": {
					"builder": "@nguniversal/builders:ssr-dev-server",
					"configurations": {
						"development": {
							"browserTarget": "environment-variables-ssr-example:build:development",
							"serverTarget": "environment-variables-ssr-example:server:development"
						},
						"production": {
							"browserTarget": "environment-variables-ssr-example:build:production",
							"serverTarget": "environment-variables-ssr-example:server:production"
						}
					},
					"defaultConfiguration": "development"
				},
				"prerender": {
					"builder": "@nguniversal/builders:prerender",
					"options": {
						"routes": ["/"]
					},
					"configurations": {
						"production": {
							"browserTarget": "environment-variables-ssr-example:build:production",
							"serverTarget": "environment-variables-ssr-example:server:production"
						},
						"development": {
							"browserTarget": "environment-variables-ssr-example:build:development",
							"serverTarget": "environment-variables-ssr-example:server:development"
						}
					},
					"defaultConfiguration": "production"
				}
			},
			"tags": []
		},
		"ngx-nuts-and-bolts": {
			"projectType": "library",
			"root": "libs/ngx-nuts-and-bolts",
			"sourceRoot": "libs/ngx-nuts-and-bolts/src",
			"prefix": "inf",
			"architect": {
				"build": {
					"builder": "@nrwl/angular:package",
					"outputs": ["dist/libs/ngx-nuts-and-bolts"],
					"options": {
						"project": "libs/ngx-nuts-and-bolts/ng-package.json",
						"tsConfig": "libs/ngx-nuts-and-bolts/tsconfig.lib.json"
					},
					"configurations": {
						"production": {
							"tsConfig": "libs/ngx-nuts-and-bolts/tsconfig.lib.prod.json"
						},
						"development": {
							"tsConfig": "libs/ngx-nuts-and-bolts/tsconfig.lib.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/libs/ngx-nuts-and-bolts"],
					"options": {
						"jestConfig": "libs/ngx-nuts-and-bolts/jest.config.ts",
						"passWithNoTests": true
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["libs/ngx-nuts-and-bolts/src/**/*.ts", "libs/ngx-nuts-and-bolts/src/**/*.html"]
					}
				},
				"storybook": {
					"builder": "@storybook/angular:start-storybook",
					"options": {
						"port": 4400,
						"configDir": "libs/ngx-nuts-and-bolts/.storybook",
						"browserTarget": "ngx-nuts-and-bolts:build-storybook",
						"compodoc": false
					},
					"configurations": {
						"ci": {
							"quiet": true
						}
					}
				},
				"build-storybook": {
					"builder": "@storybook/angular:build-storybook",
					"outputs": ["{options.outputPath}"],
					"options": {
						"outputDir": "dist/storybook/ngx-nuts-and-bolts",
						"configDir": "libs/ngx-nuts-and-bolts/.storybook",
						"browserTarget": "ngx-nuts-and-bolts:build-storybook",
						"compodoc": false
					},
					"configurations": {
						"ci": {
							"quiet": true
						}
					}
				}
			},
			"tags": []
		},
		"ngx-nuts-and-bolts-ssr": {
			"projectType": "library",
			"root": "libs/ngx-nuts-and-bolts-ssr",
			"sourceRoot": "libs/ngx-nuts-and-bolts-ssr/src",
			"prefix": "ngx-nuts-and-bolts",
			"architect": {
				"build": {
					"builder": "@nrwl/angular:package",
					"outputs": ["dist/libs/ngx-nuts-and-bolts-ssr"],
					"options": {
						"project": "libs/ngx-nuts-and-bolts-ssr/ng-package.json"
					},
					"configurations": {
						"production": {
							"tsConfig": "libs/ngx-nuts-and-bolts-ssr/tsconfig.lib.prod.json"
						},
						"development": {
							"tsConfig": "libs/ngx-nuts-and-bolts-ssr/tsconfig.lib.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/libs/ngx-nuts-and-bolts-ssr"],
					"options": {
						"jestConfig": "libs/ngx-nuts-and-bolts-ssr/jest.config.ts",
						"passWithNoTests": true
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["libs/ngx-nuts-and-bolts-ssr/src/**/*.ts", "libs/ngx-nuts-and-bolts-ssr/src/**/*.html"]
					}
				}
			},
			"tags": []
		}
	}
}
