{
	"name": "ngx-nuts-and-bolts",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"ng": "nx",
		"postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
		"start": "nx serve ngx-nuts-and-bolts-demo",
		"build": "nx build ngx-nuts-and-bolts && npm run copy-license",
		"copy-license": "cp ./LICENSE ./dist/libs/ngx-nuts-and-bolts",
		"test": "nx run-many --target=test --all --parallel --code-coverage",
		"test:watch": "nx run-many --target=test --all --parallel --code-coverate --watch",
		"precommit": "concurrently \"npm run precommit:tsc\" \"npm run precommit:lint\"",
		"precommit:tsc": "concurrently \"npm run tsc:lib\" \"npm run tsc:spec\"",
		"precommit:lint": "nx affected:lint --parallel --uncommitted",
		"tsc:lib": "tsc --noEmit -p ./libs/ngx-nuts-and-bolts/tsconfig.lib.json",
		"tsc:spec": "tsc --noEmit -p ./libs/ngx-nuts-and-bolts/tsconfig.spec.json",
		"prepare": "husky install"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^12.2.0",
		"@angular/common": "^12.2.0",
		"@angular/compiler": "^12.2.0",
		"@angular/core": "^12.2.0",
		"@angular/forms": "^12.2.0",
		"@angular/platform-browser": "^12.2.0",
		"@angular/platform-browser-dynamic": "^12.2.0",
		"@angular/router": "^12.2.0",
		"@nrwl/angular": "13.1.3",
		"rxjs": "~6.6.0",
		"tslib": "^2.0.0",
		"zone.js": "~0.11.4"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~12.2.0",
		"@angular-eslint/eslint-plugin": "~12.3.0",
		"@angular-eslint/eslint-plugin-template": "~12.3.0",
		"@angular-eslint/template-parser": "~12.3.0",
		"@angular/cli": "~12.2.0",
		"@angular/compiler-cli": "^12.2.0",
		"@angular/language-service": "^12.2.0",
		"@nrwl/cli": "13.1.3",
		"@nrwl/cypress": "13.1.3",
		"@nrwl/eslint-plugin-nx": "13.1.3",
		"@nrwl/jest": "13.1.3",
		"@nrwl/linter": "13.1.3",
		"@nrwl/nx-cloud": "latest",
		"@nrwl/tao": "13.1.3",
		"@nrwl/workspace": "13.1.3",
		"@types/jest": "27.0.2",
		"@types/node": "14.14.33",
		"@typescript-eslint/eslint-plugin": "~4.33.0",
		"@typescript-eslint/parser": "~4.33.0",
		"concurrently": "^6.3.0",
		"cypress": "^8.3.0",
		"eslint": "7.32.0",
		"eslint-config-prettier": "8.1.0",
		"eslint-plugin-cypress": "^2.10.3",
		"eslint-plugin-rxjs": "^3.3.7",
		"husky": "^7.0.4",
		"jest": "27.2.3",
		"jest-preset-angular": "10.0.1",
		"ng-packagr": "^12.1.1",
		"postcss": "^8.3.9",
		"postcss-import": "^14.0.2",
		"postcss-preset-env": "^6.7.0",
		"postcss-url": "^10.1.1",
		"prettier": "^2.3.1",
		"ts-jest": "27.0.5",
		"typescript": "~4.3.5"
	}
}
