{
	"name": "ngx-nuts-and-bolts",
	"version": "0.0.0",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/infinum/ngx-nuts-and-bolts.git"
	},
	"scripts": {
		"ng": "nx",
		"start": "nx run ngx-nuts-and-bolts:storybook",
		"start:environment-variables-ssr-example": "NGX_NUTS_AND_BOLTS_EXAMPLE_FOO='I am Foo' NGX_NUTS_AND_BOLTS_EXAMPLE_BAR='I am Bar' nx run environment-variables-ssr-example:serve-ssr",
		"start:environment-variables-fetch-example": "nx run environment-variables-fetch-example:serve",
		"start:breadcrumbs-example": "nx run breadcrumbs-example:serve",
		"build": "concurrently \"npm run build:core\" \"npm run build:ssr\"",
		"build:core": "nx build ngx-nuts-and-bolts --skip-nx-cache && npm run copy-files:core && npm run build:schematics",
		"build:ssr": "nx build ngx-nuts-and-bolts-ssr --skip-nx-cache && npm run copy-files:ssr",
		"build:schematics": "nx build ngx-nuts-and-bolts/schematics --skip-nx-cache",
		"copy-files:core": "cp ./LICENSE ./dist/libs/ngx-nuts-and-bolts/LICENSE && cp ./README.md ./dist/libs/ngx-nuts-and-bolts/README.md",
		"copy-files:ssr": "cp ./LICENSE ./dist/libs/ngx-nuts-and-bolts-ssr/LICENSE && cp ./README.md ./dist/libs/ngx-nuts-and-bolts-ssr/README.md",
		"test": "nx run-many --target=test --all --parallel --skip-nx-cache",
		"test:watch": "nx run-many --target=test --all --parallel --watchAll --skip-nx-cache",
		"dev:ssr-example": "ng run environment-variables-ssr-example:serve-ssr",
		"serve:ssr-example": "node dist/environment-variables-ssr-example/server/main.js",
		"build:ssr-example": "ng build && ng run environment-variables-ssr-example:server",
		"prerender": "ng run environment-variables-ssr-example:prerender",
		"prepare": "husky install"
	},
	"private": false,
	"dependencies": {
		"@angular/animations": "20.1.8",
		"@angular/common": "20.1.8",
		"@angular/compiler": "20.1.8",
		"@angular/core": "20.1.8",
		"@angular/forms": "20.1.8",
		"@angular/platform-browser": "20.1.8",
		"@angular/platform-browser-dynamic": "20.1.8",
		"@angular/platform-server": "20.1.8",
		"@angular/router": "20.1.8",
		"@angular/ssr": "20.1.6",
		"@nx/angular": "21.4.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rxjs": "~7.8.0",
		"storybook": "9.1.3",
		"tslib": "^2.3.0",
		"zone.js": "0.15.1"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "20.1.6",
		"@angular-devkit/core": "20.1.6",
		"@angular-devkit/schematics": "20.1.6",
		"@angular-eslint/eslint-plugin": "20.2.0",
		"@angular-eslint/eslint-plugin-template": "20.2.0",
		"@angular-eslint/template-parser": "20.2.0",
		"@angular/cli": "20.1.6",
		"@angular/compiler-cli": "20.1.8",
		"@angular/language-service": "20.1.8",
		"@nx/cypress": "21.4.1",
		"@nx/eslint": "21.4.1",
		"@nx/eslint-plugin": "21.4.1",
		"@nx/jest": "21.4.1",
		"@nx/js": "21.4.1",
		"@nx/storybook": "21.4.1",
		"@nx/web": "21.4.1",
		"@nx/workspace": "21.4.1",
		"@schematics/angular": "20.1.6",
		"@storybook/angular": "9.1.3",
		"@types/jest": "29.5.14",
		"@types/node": "18.16.9",
		"@typescript-eslint/eslint-plugin": "7.18.0",
		"@typescript-eslint/parser": "7.18.0",
		"@typescript-eslint/utils": "^7.16.0",
		"autoprefixer": "^10.4.0",
		"browser-sync": "^3.0.0",
		"concurrently": "^8.2.0",
		"cypress": "14.5.4",
		"eslint": "8.57.1",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-cypress": "2.15.2",
		"eslint-plugin-jasmine": "^4.1.3",
		"eslint-plugin-rxjs": "^5.0.3",
		"husky": "^8.0.3",
		"jest": "29.7.0",
		"jest-environment-jsdom": "29.7.0",
		"jest-fail-on-console": "^3.1.1",
		"lint-staged": "^13.2.3",
		"msw": "^2.0.11",
		"ng-packagr": "20.1.0",
		"nx": "21.4.1",
		"postcss-preset-env": "^8.5.1",
		"prettier": "^2.6.2",
		"ts-jest": "29.4.1",
		"ts-node": "10.9.1",
		"typescript": "5.8.3",
		"undici": "^5.28.3",
		"eslint-plugin-storybook": "9.1.3",
		"@storybook/addon-docs": "9.1.3",
		"ts-jest": "29.4.1"
	},
	"msw": {
		"workerDirectory": "libs/msw-public"
	}
}
