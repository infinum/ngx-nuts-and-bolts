{
  "name": "ngx-nuts-and-bolts",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/infinum/ngx-nuts-and-bolts.git"
  },
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx run ngx-nuts-and-bolts:storybook",
    "start:environment-variables-ssr-example": "NGX_NUTS_AND_BOLTS_EXAMPLE_FOO='I am Foo' NGX_NUTS_AND_BOLTS_EXAMPLE_BAR='I am Bar' nx run environment-variables-ssr-example:serve-ssr",
    "start:environment-variables-fetch-example": "nx run environment-variables-fetch-example:serve",
    "build": "concurrently \"npm run build:core\" \"npm run build:ssr\"",
    "build:core": "nx build ngx-nuts-and-bolts --skip-nx-cache && npm run copy-files:core",
    "build:ssr": "nx build ngx-nuts-and-bolts-ssr --skip-nx-cache && npm run copy-files:ssr",
    "copy-files:core": "cp ./LICENSE ./dist/libs/ngx-nuts-and-bolts/LICENSE && cp ./README.md ./dist/libs/ngx-nuts-and-bolts/README.md",
    "copy-files:ssr": "cp ./LICENSE ./dist/libs/ngx-nuts-and-bolts-ssr/LICENSE && cp ./README.md ./dist/libs/ngx-nuts-and-bolts-ssr/README.md",
    "test": "nx run-many --target=test --all --parallel --skip-nx-cache",
    "test:watch": "nx run-many --target=test --all --parallel --watchAll --skip-nx-cache",
    "precommit": "concurrently \"npm run precommit:tsc\" \"npm run precommit:lint\"",
    "precommit:tsc": "concurrently \"npm run tsc:lib\" \"npm run tsc:spec\"",
    "precommit:lint": "nx affected:lint --parallel --uncommitted",
    "tsc:lib": "tsc --noEmit -p ./libs/ngx-nuts-and-bolts/tsconfig.lib.json",
    "tsc:spec": "tsc --noEmit -p ./libs/ngx-nuts-and-bolts/tsconfig.spec.json",
    "dev:ssr-example": "ng run environment-variables-ssr-example:serve-ssr",
    "serve:ssr-example": "node dist/environment-variables-ssr-example/server/main.js",
    "build:ssr-example": "ng build && ng run environment-variables-ssr-example:server",
    "prerender": "ng run environment-variables-ssr-example:prerender",
    "prepare": "husky install"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/cdk": "^13.3.1",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/material": "^13.3.1",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/platform-server": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@nguniversal/express-engine": "^13.1.0",
    "@nrwl/angular": "^13.9.4",
    "express": "^4.17.3",
    "rxjs": "~7.5.5",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.11",
    "@angular-eslint/eslint-plugin": "^13.1.0",
    "@angular-eslint/eslint-plugin-template": "^13.1.0",
    "@angular-eslint/template-parser": "^13.1.0",
    "@angular/cli": "^14.2.11",
    "@angular/compiler-cli": "^14.3.0",
    "@angular/language-service": "^14.3.0",
    "@infinumjs/eslint-config-angular-ts": "^3.1.0",
    "@nguniversal/builders": "^14.2.3",
    "@nrwl/cli": "^13.9.4",
    "@nrwl/cypress": "^13.9.4",
    "@nrwl/eslint-plugin-nx": "^13.9.4",
    "@nrwl/jest": "^13.9.4",
    "@nrwl/linter": "^13.9.4",
    "@nrwl/nx-cloud": "^13.1.6",
    "@nrwl/storybook": "^13.9.4",
    "@nrwl/tao": "^13.9.4",
    "@nrwl/workspace": "13.9.4",
    "@storybook/addon-essentials": "~6.4.19",
    "@storybook/angular": "~6.4.19",
    "@storybook/builder-webpack5": "~6.4.19",
    "@storybook/manager-webpack5": "~6.4.19",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/parser": "^5",
    "concurrently": "^7.0.0",
    "cypress": "^9.5.2",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-rxjs": "^5.0.2",
    "husky": "^7.0.4",
    "jest": "27.5.1",
    "jest-fail-on-console": "^2.3.0",
    "jest-preset-angular": "11.1.1",
    "ng-packagr": "^14.2.2",
    "postcss": "^8.4.12",
    "postcss-import": "^14.1.0",
    "postcss-preset-env": "^7.4.3",
    "postcss-url": "^10.1.3",
    "prettier": "2.6.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "ts-jest": "27.1.3",
    "typescript": "~4.6.2"
  }
}